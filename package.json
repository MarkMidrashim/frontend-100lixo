{
  "name": "100lixo-frontend",
  "version": "1.0.0",
  "author": {
    "name": "Marcos Franco",
    "email": "marcosfranco.developer@gmail.com"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve ngx-100lixo --disable-host-check --host 0.0.0.0 --proxy-config proxy.conf.json",
    "build:initial": "npm i && npm run build:libs && npm run build:app",
    "build:app": "ng build ngx-100lixo",
    "build:libs": "npm run build:lib-ngx-domain && npm run build:lib-ngx-api && npm run build:lib-ngx-auth && npm run build:lib-ngx-service && npm run build:lib-ngx-pipes && npm run build:lib-ngx-component",
    "build:lib-ngx-domain": "ng build @100lixo-lib/ngx-domain",
    "build:lib-ngx-api": "ng build @100lixo-lib/ngx-api",
    "build:lib-ngx-auth": "ng build @100lixo-lib/ngx-auth",
    "build:lib-ngx-component": "ng build @100lixo-lib/ngx-component",
    "build:lib-ngx-service": "ng build @100lixo-lib/ngx-service",
    "build:lib-ngx-pipes": "ng build @100lixo-lib/ngx-pipes",
    "test:coverage": "npm run test:coverage:libs && npm run test:coverage:ngx-100lixo",
    "test:coverage:libs": "npm run test:coverage:lib-ngx-domain && npm run test:coverage:lib-ngx-api && npm run test:coverage:lib-ngx-component && npm run test:coverage:lib-ngx-service && npm run test:coverage:lib-ngx-pipes",
    "test:coverage:lib-ngx-domain": "ng test @100lixo-lib/ngx-domain",
    "test:coverage:lib-ngx-api": "ng test @100lixo-lib/ngx-api",
    "test:coverage:lib-ngx-component": "ng test @100lixo-lib/ngx-component",
    "test:coverage:lib-ngx-service": "ng test @100lixo-lib/ngx-service",
    "test:coverage:lib-ngx-pipes": "ng test @100lixo-lib/ngx-pipes",
    "test:coverage:ngx-100lixo": "ng test ngx-100lixo --coverage",
    "clear": "rm -rf node_modules/ && rm -rf dist/ && rm package-lock.json && npm clear cache --force",
    "fake-api": "json-server --delay 500 db.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ngcc"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.14",
    "@angular/common": "~11.2.14",
    "@angular/compiler": "~11.2.14",
    "@angular/core": "~11.2.14",
    "@angular/elements": "~11.2.14",
    "@angular/forms": "~11.2.14",
    "@angular/localize": "^11.2.14",
    "@angular/platform-browser": "~11.2.14",
    "@angular/platform-browser-dynamic": "~11.2.14",
    "@angular/router": "~11.2.14",
    "@briebug/jest-schematic": "^3.1.0",
    "@ng-bootstrap/ng-bootstrap": "^9.1.3",
    "@popperjs/core": "^2.9.2",
    "bootstrap": "^5.0.2",
    "cpf-cnpj-validator": "^1.0.3",
    "crypto-js": "^4.1.1",
    "json-server": "^0.17.0",
    "jwt-decode": "^3.1.2",
    "ngx-cookie-service": "^11.0.2",
    "ngx-infinite-scroll": "^10.0.1",
    "ngx-mask": "^11.1.5",
    "ngx-perfect-scrollbar": "^10.1.1",
    "ngx-skeleton-loader": "^4.0.0",
    "rxjs": "~6.6.0",
    "smooth-scrollbar": "^8.7.0",
    "sweetalert2": "^11.0.19",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-builders/jest": "latest",
    "@angular-devkit/build-angular": "~0.1102.14",
    "@angular/cdk": "~11.2.13",
    "@angular/cli": "~11.2.14",
    "@angular/compiler-cli": "~11.2.14",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@types/crypto-js": "^4.1.1",
    "@types/jasmine": "~3.6.0",
    "@types/jest": "latest",
    "@types/node": "^12.11.1",
    "@types/uuid": "^8.3.3",
    "codelyzer": "^6.0.0",
    "husky": "^7.0.2",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jest": "latest",
    "karma-coverage": "~2.0.3",
    "lint-staged": "^11.1.2",
    "ng-packagr": "^11.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5"
  },
  "lint-staged": {
    ".css": "stylelint --fix",
    ".scss": "stylelint --sintax=scss --fix",
    ".ts": "tslint --fix"
  }
}
